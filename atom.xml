---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Factory Design Labs :: Screwdriver</title>
  <link href="http://screwdriver.factorylabs.com/atom.xml" rel="self"/>
  <link href="http://screwdriver.factorylabs.com/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://screwdriver.factorylabs.com/</id>
  <author>
    <name>Factory Design Labs</name>
    <email>interactive@factorylabs.com</email>
  </author>
  {% for post in site.posts limit:10 %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://screwdriver.factorylabs.com/{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://screwdriver.factorylabs.com/{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
    <author>
      <name>Factory Design Labs</name>
      <uri>http://www.factorylabs.com/</uri>
    </author>
  </entry>
  {% endfor %}
</feed>